   "source": [
    "# Thanks to the first cte, I extracted from the days table, each day of June 2020.\n",
    "# Thanks to cte2, I extracted from the customers table, the number of churn that occured each day of the month\n",
    "# I finally LEFT JOINed cte with cte2, in order to also have dates when no churn occured (using COALESCE\n",
    "# to overwrite null values).\n",
    "\n",
    "day_june_churn = pd.read_sql('WITH cte AS(\\\n",
    "    SELECT strftime(\"%d\", DATE_DAY) as days\\\n",
    "    FROM days\\\n",
    "    WHERE DATE_DAY BETWEEN \"2020-06-01\" AND \"2020-06-31\"),\\\n",
    "    cte2 AS(\\\n",
    "    SELECT strftime(\"%d\", END_DATE) as days, COUNT(CUSTOMER_ID) AS COUNT\\\n",
    "    FROM customers \\\n",
    "    WHERE END_DATE BETWEEN \"2020-06-01\" AND \"2020-06-31\" \\\n",
    "    AND MONTHLY_AMOUNT != 0 AND START_DATE != END_DATE \\\n",
    "    GROUP BY days)\\\n",
    "    SELECT cte.days, COALESCE(COUNT, 0) AS COUNT\\\n",
    "    FROM cte \\\n",
    "    LEFT JOIN cte2 \\\n",
    "    ON cte.days = cte2.days \\\n",
    "    ORDER BY cte.days', conn)\n",
    "\n",
    "\n",
    "day_june_churn.head(31)"
   ]
